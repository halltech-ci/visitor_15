<odoo>
  <data>
    <record model="ir.ui.view" id="hta_fleet_mgmnt_vehicle_form_view">
        <field name="name">fleet_mgmnt_vehicle_form_view</field>
        <field name="model">fleet.vehicle</field>
        <field name="inherit_id" ref="fleet.fleet_vehicle_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet/group/group[2]/field[@name='acquisition_date']" position="before">
                <field name="partner_id"/>
            </xpath>
        </field>
    </record>
  </data>
</odoo>